{
  "emulators": {
    "dataconnect": {
      "dataDir": "dataconnect/.dataconnect/pgliteData"
    }
  },
  "dataconnect": {
    "source": "dataconnect"
  },
  "storage": {
    "rules": "storage.rules",
    "cors": [
      {
        "origin": ["http://127.0.0.1:3000"],
        "method": ["GET", "HEAD", "PUT", "POST", "DELETE"],
        "maxAgeSeconds": 3600
      }
    ]
  }
}